{
  "name": "@lawlzer/helpers",
  "version": "2.0.8",
  "description": "The default Helpers file for any of Lawlzer's coding projects. ",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only .\\src\\index.ts",
    "build": "rimraf ./dist &&  npm run build:js && npm run build:types",
    "build:types": "tsc",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps true",
    "publish": "rimraf ./dist && npm run build && npm publish --access public && echo \"Successfully published to NPM!\""
  },
  "files": [
    "src",
    "dist"
  ],
  "author": "Lawlzer",
  "license": "ISC",
  "dependencies": {
    "ms": "^2.1.3"
  },
  "publishConfig": {
    "@YOUR-USERNAME:registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.0.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.5",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-packagejson": "^2.2.18",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  }
}
